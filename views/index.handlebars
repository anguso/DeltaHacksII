<div class="container-fluid">
  <div class="page-header">
    <h1>Find your MP</h1>
  </div>
  <form class="navbar-form navbar-left" role="search" method="POST" action="/">
    <div class="form-group">
      <input name="given_name" type="text" class="form-control" placeholder="First name">
      <input name="family_name" type="text" class="form-control" placeholder="Last name">
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
  </form>
  <div class="list-group col-md-6">
    {{#each members}}
      <a href="{{this.url}}" class="list-group-item">{{this.name}}</a>
    {{/each}}
  </div>
</div>
